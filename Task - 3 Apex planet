    <div id="carousel">
      <img id="slide"
        src="data:image/jpeg;base64,"
        alt="Slide">
    </div>
    <br>
    <button onclick="prevSlide()">Prev</button>
    <button onclick="nextSlide()">Next</button>
  </section>



