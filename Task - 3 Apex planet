/* QUIZ LOGIC */
let currentQuestion = 0;
let score = 0;
const quiz = [
  { q: "What does HTML stand for? (A) Hyper Text Markup Language (B) Home Tool Markup Language (C) Hyperlinks Text Markup Language", answer: "A" },
  { q: "What is the capital of France? (A) Rome (B) Paris (C) Berlin", answer: "B" }
];

function showQuestion() {
  document.getElementById("question").textContent = quiz[currentQuestion].q;
}

function checkAnswer(option) {
  if (option === quiz[currentQuestion].answer) {
    score++;
    document.getElementById("result").textContent = "Correct!";
  } else {
    document.getElementById("result").textContent = "Wrong!";
  }
  currentQuestion++;
  if (currentQuestion < quiz.length) {
    showQuestion();
  } else {
    document.getElementById("result").textContent += ` Final Score: ${score}`;
  }
}

showQuestion();

/* IMAGE CAROUSEL LOGIC */
let index = 0;
const images = [
  "data:image/jpeg;base64,",
  "data:image/jpeg;base64,",
  "data:image/jpeg;base64,"
];

function showSlide() {
  document.getElementById("slide").src = images[index];
}

function nextSlide() {
  index = (index + 1) % images.length;
  showSlide();
}

function prevSlide() {
  index = (index - 1 + images.length) % images.length;
  showSlide();
}


