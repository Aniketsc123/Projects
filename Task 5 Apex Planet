/* ================ Utilities ================ */
const qs = (s, r = document) => r.querySelector(s);
const qsa = (s, r = document) => [...r.querySelectorAll(s)];
const money = n => `‚Çπ${n.toLocaleString('en-IN')}`;
const save = (k, v) => localStorage.setItem(k, JSON.stringify(v));
const load = (k, d) => JSON.parse(localStorage.getItem(k) || JSON.stringify(d));

/* ================ State ================ */
const state = {
  cart: load('gg_cart', {}) // {id: qty}
};

/* ================ Elements ================ */
const grid = qs('#grid');
const search = qs('#search');
const clearSearch = qs('#clearSearch');
const priceRange = qs('#priceRange');
const priceValue = qs('#priceValue');
const sortSelect = qs('#sortSelect');
const tabs = qsa('.tab');

const cartBtn = qs('#cartBtn');
const cartDrawer = qs('#cartDrawer');
const closeCart = qs('#closeCart');
const backdrop = qs('#backdrop');
const cartCount = qs('#cartCount');
const cartItems = qs('#cartItems');
const subtotalEl = qs('#subtotal');
const checkoutBtn = qs('#checkout');
const toast = qs('#toast');

/* ================ Rendering ================ */
function card(p) {
  return `
  <article class="card" data-id="${p.id}">
    <div class="media">
      <img src="${p.img}" alt="${p.name}" loading="lazy" decoding="async" width="800" height="600">
    </div>
    <div class="body">
      <h3>${p.name}</h3>
      <div class="meta">
        <span>${p.cat}</span>
        <span>‚òÖ ${p.rating.toFixed(1)}</span>
      </div>
      <div class="actions">
        <div>
          <span class="price">${money(p.price)}</span>
          <span class="mrp">${money(p.mrp)}</span>
        </div>
        <button class="btn add">Add</button>
      </div>
    </div>
  </article>`;
}

function render() {
  const term = search.value.trim().toLowerCase();
  const activeTab = qs('.tab.active')?.dataset.cat || 'all';
  const maxPrice = +priceRange.value;
  const sorterMap = {
    'pop': (a, b) => b.pop - a.pop,
    'price-asc': (a, b) => a.price - b.price,
    'price-desc': (a, b) => b.price - a.price,
    'name-asc': (a, b) => a.name.localeCompare(b.name),
    'rating-desc': (a, b) => b.rating - a.rating,
  };
  const sortFn = sorterMap[sortSelect.value] || sorterMap['pop'];

  const items = PRODUCTS
    .filter(p => (activeTab === 'all' || p.cat === activeTab))
    .filter(p => p.price <= maxPrice)
    .filter(p => p.name.toLowerCase().includes(term))
    .sort(sortFn);

  grid.setAttribute('aria-busy', 'true');
  grid.innerHTML = items.map(card).join('');
  grid.setAttribute('aria-busy', 'false');

  updateSliderBG(); // keep gradient responsive to value
}

/* ================ Slider gradient + label ================ */
function updateSliderBG() {
  const val = +priceRange.value;
  const min = +priceRange.min, max = +priceRange.max;
  const pct = ((val - min) / (max - min)) * 100;
  priceValue.textContent = money(val);
  priceRange.style.background = `linear-gradient(90deg, var(--leaf) ${pct}%, var(--soil) ${pct}%)`;
}

/* ================ Cart logic ================ */
function addToCart(id) {
  state.cart[id] = (state.cart[id] || 0) + 1;
  save('gg_cart', state.cart);
  updateCartUI();
  bumpBadge();
  showToast('Added to basket');
}

function removeFromCart(id) {
  delete state.cart[id];
  save('gg_cart', state.cart);
  updateCartUI();
}

function setQty(id, qty) {
  if (qty <= 0) return removeFromCart(id);
  state.cart[id] = qty;
  save('gg_cart', state.cart);
  updateCartUI();
}

function cartLines() {
  return Object.entries(state.cart).map(([id, qty]) => {
    const p = PRODUCTS.find(x => x.id === id);
    return { ...p, qty, line: p.price * qty };
  });
}

function updateCartUI() {
  const lines = cartLines();
  cartCount.textContent = lines.reduce((a, x) => a + x.qty, 0);

  cartItems.innerHTML = lines.length ? lines.map(l => `
    <div class="item" data-id="${l.id}">
      <img src="${l.img}" alt="${l.name}" loading="lazy" decoding="async" width="128" height="128">
      <div>
        <strong>${l.name}</strong>
        <div class="meta"><span>${l.cat}</span><span>${money(l.price)} each</span></div>
        <div class="qty">
          <button class="dec" aria-label="Decrease">‚àí</button>
          <span>${l.qty}</span>
          <button class="inc" aria-label="Increase">Ôºã</button>
        </div>
      </div>
      <div style="text-align:right">
        <div><strong>${money(l.line)}</strong></div>
        <button class="remove">Remove</button>
      </div>
    </div>
  `).join('') : `<p class="meta">Your basket is empty.</p>`;

  const subtotal = lines.reduce((a, x) => a + x.line, 0);
  subtotalEl.textContent = money(subtotal);
}

/* ================ UI Animations ================ */
function bumpBadge() {
  cartCount.animate(
    [{ transform: 'scale(1)' }, { transform: 'scale(1.25)' }, { transform: 'scale(1)' }],
    { duration: 300, easing: 'cubic-bezier(.2,.7,.2,1)' }
  );
}

function showToast(text) {
  toast.textContent = text;
  toast.hidden = false;
  toast.animate(
    [{ transform: 'translate(-50%, 10px)', opacity: 0 },
    { transform: 'translate(-50%, 0)', opacity: 1, offset: .2 },
    { transform: 'translate(-50%, 0)', opacity: 1, offset: .8 },
    { transform: 'translate(-50%, 10px)', opacity: 0 }],
    { duration: 1600, easing: 'ease' }
  ).onfinish = () => toast.hidden = true;
}

/* Fly image to cart icon (subtle, cross-browser safe) */
function flyToCart(imgEl) {
  const cartIcon = cartBtn;
  if (!imgEl || !cartIcon || !imgEl.getBoundingClientRect) return;

  const start = imgEl.getBoundingClientRect();
  const end = cartIcon.getBoundingClientRect();
  const ghost = imgEl.cloneNode();
  Object.assign(ghost.style, {
    position: 'fixed', left: `${start.left}px`, top: `${start.top}px`,
    width: `${start.width}px`, height: `${start.height}px`, borderRadius: '10px',
    zIndex: 60, pointerEvents: 'none', boxShadow: '0 8px 20px rgba(0,0,0,.25)'
  });
  document.body.appendChild(ghost);

  ghost.animate([
    { transform: 'translate(0,0) scale(1)', opacity: .9 },
    { transform: `translate(${end.left - start.left}px, ${end.top - start.top}px) scale(.2)`, opacity: 0.2 }
  ], { duration: 600, easing: 'cubic-bezier(.2,.7,.2,1)' }).onfinish = () => ghost.remove();
}

/* ================ Drawer ================ */
function openCart() { cartDrawer.classList.add('open'); cartDrawer.setAttribute('aria-hidden', 'false'); backdrop.hidden = false; }
function closeCartDrawer() { cartDrawer.classList.remove('open'); cartDrawer.setAttribute('aria-hidden', 'true'); backdrop.hidden = true; }

/* ================ Events ================ */
document.addEventListener('click', (e) => {
  // Add button
  if (e.target.classList.contains('add')) {
    const card = e.target.closest('.card');
    const id = card?.dataset.id;
    if (id) {
      const img = card.querySelector('img');
      flyToCart(img);
      addToCart(id);
    }
  }

  // Tabs
  if (e.target.classList.contains('tab')) {
    qsa('.tab').forEach(t => t.classList.remove('active'));
    e.target.classList.add('active');
    render();
  }

  // Drawer
  if (e.target === cartBtn) openCart();
  if (e.target === closeCart || e.target === backdrop) closeCartDrawer();

  // Cart item actions
  const item = e.target.closest('.item');
  if (item) {
    const id = item.dataset.id;
    if (e.target.classList.contains('inc')) setQty(id, (state.cart[id] || 1) + 1);
    if (e.target.classList.contains('dec')) setQty(id, (state.cart[id] || 1) - 1);
    if (e.target.classList.contains('remove')) removeFromCart(id);
  }
});

// Search / Sort / Price
['input', 'change'].forEach(evt => {
  search.addEventListener(evt, render);
  sortSelect.addEventListener(evt, render);
  priceRange.addEventListener(evt, render);
});
clearSearch.addEventListener('click', () => { search.value = ''; render(); });

checkoutBtn.addEventListener('click', () => {
  const subtotal = cartLines().reduce((a, x) => a + x.line, 0);
  if (!subtotal) return alert('Your basket is empty.');
  alert(`‚úÖ Order placed!\nSubtotal: ${money(subtotal)}\n(Checkout simulated in demo)`);
  state.cart = {}; save('gg_cart', state.cart); updateCartUI(); closeCartDrawer();
});

/* ================ Boot ================ */
function boot() {
  updateSliderBG();
  render();
  updateCartUI();
}
boot();
