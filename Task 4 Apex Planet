/** Main JS for portfolio / todo / products
 *  - To-Do uses localStorage
 *  - Products are rendered from JS data with filter/sort/search
 *  - Small theme toggle
 */

/* ---------------------------
   Helpers
-----------------------------*/
const $ = (sel, p = document) => p.querySelector(sel);
const $$ = (sel, p = document) => Array.from(p.querySelectorAll(sel));

/* ---------------------------
   Theme Toggle
-----------------------------*/
const themeToggle = document.getElementById('themeToggle');
themeToggle.addEventListener('click', () => {
  const isLight = document.documentElement.getAttribute('data-theme') === 'light';
  document.documentElement.setAttribute('data-theme', isLight ? 'dark' : 'light');
  themeToggle.textContent = isLight ? '🌙' : '☀️';
});

/* ---------------------------
   To-Do App
-----------------------------*/
const TODO_KEY = 'my_todos_v1';
const todoForm = $('#todoForm');
const todoInput = $('#todoInput');
const todoListEl = $('#todoList');
const clearDoneBtn = $('#clearDone');
const clearAllBtn = $('#clearAll');

let todos = JSON.parse(localStorage.getItem(TODO_KEY) || '[]');

function saveTodos() {
  localStorage.setItem(TODO_KEY, JSON.stringify(todos));
}

function renderTodos() {
  todoListEl.innerHTML = '';
  if (!todos.length) {
    const hint = document.createElement('div');
    hint.className = 'muted';
    hint.textContent = 'No tasks yet — add something above!';
    todoListEl.appendChild(hint);
    return;
  }
  todos.forEach(todo => {
    const li = document.createElement('li');
    li.className = 'todo-item';
    li.dataset.id = todo.id;

    const checkbox = document.createElement('input');
    checkbox.type = 'checkbox';
    checkbox.checked = todo.done;
    checkbox.addEventListener('change', () => {
      todo.done = checkbox.checked;
      if (todo.done) li.classList.add('completed'); else li.classList.remove('completed');
      saveTodos();
    });

    const span = document.createElement('div');
    span.className = 'task-text';
    span.textContent = todo.text;

    const del = document.createElement('button');
    del.setAttribute('aria-label', 'Delete task');
    del.innerHTML = '🗑';
    del.addEventListener('click', () => removeTodo(todo.id, li));

    if (todo.done) li.classList.add('completed');
    li.append(checkbox, span, del);
    todoListEl.appendChild(li);

    // simple appear animation
    requestAnimationFrame(() => li.style.transform = 'translateY(0)');
    li.style.transform = 'translateY(6px)';
  });
}

function addTodo(text) {
  const id = Date.now().toString(36) + Math.random().toString(36).slice(2, 6);
  todos.unshift({ id, text: text.trim(), done: false });
  saveTodos(); renderTodos();
}

function removeTodo(id, liElement) {
  // animate out
  liElement.classList.add('removing');
  setTimeout(() => {
    todos = todos.filter(t => t.id !== id);
    saveTodos();
    renderTodos();
  }, 250);
}

todoForm.addEventListener('submit', (e) => {
  e.preventDefault();
  const val = todoInput.value;
  if (!val.trim()) return;
  addTodo(val);
  todoInput.value = '';
  todoInput.focus();
});

clearDoneBtn.addEventListener('click', () => {
  todos = todos.filter(t => !t.done);
  saveTodos(); renderTodos();
});
clearAllBtn.addEventListener('click', () => {
  if (!confirm('Clear all tasks?')) return;
  todos = [];
  saveTodos(); renderTodos();
});

// initial render
renderTodos();

/* ---------------------------
   Products: data + UI
-----------------------------*/

const products = [
  { id: 1, name: 'Aurora Headphones', price: 2790, category: 'Audio', img: 'data:image/jpeg;base64,', featured: true },
  { id: 2, name: 'Nebula Lamp', price: 1299, category: 'Home', img: 'data:image/jpeg;base64,', featured: true },
  { id: 3, name: 'Orbit Mug', price: 499, category: 'Home', img: 'data:image/jpeg;base64,', featured: false },
  { id: 4, name: 'Pulse Mouse', price: 1199, category: 'Gadgets', img: 'data:image/jpeg;base64,', featured: false },
  { id: 5, name: 'Lumen Desk Mat', price: 699, category: 'Gadgets', img: 'data:image/jpeg;base64,', featured: false },
  { id: 6, name: 'Vega Keyboard', price: 3499, category: 'Gadgets', img: 'data:image/jpeg;base64,', featured: true },
  { id: 7, name: 'Canvas Backpack', price: 1999, category: 'Lifestyle', img: 'data:image/jpeg;base64,', featured: false },
  { id: 8, name: 'Solar Bottle', price: 899, category: 'Lifestyle', img: 'data:image/jpeg;base64,', featured: false }
];

const productGrid = $('#productGrid');
const categorySelect = $('#categorySelect');
const sortSelect = $('#sortSelect');
const searchInput = $('#searchInput');

// populate categories
const cats = ['all', ...Array.from(new Set(products.map(p => p.category)))];
cats.forEach(c => {
  const opt = document.createElement('option'); opt.value = c; opt.textContent = c === 'all' ? 'All' : c;
  categorySelect.appendChild(opt);
});

let productState = {
  query: '',
  category: 'all',
  sort: 'featured'
};

function renderProducts() {
  const q = productState.query.trim().toLowerCase();
  const cat = productState.category;
  let list = products.filter(p => {
    const matchQ = !q || p.name.toLowerCase().includes(q) || (p.category && p.category.toLowerCase().includes(q));
    const matchCat = cat === 'all' ? true : p.category === cat;
    return matchQ && matchCat;
  });

  // sorting
  const sort = productState.sort;
  if (sort === 'priceAsc') list.sort((a, b) => a.price - b.price);
  else if (sort === 'priceDesc') list.sort((a, b) => b.price - a.price);
  else if (sort === 'nameAsc') list.sort((a, b) => a.name.localeCompare(b.name));
  else if (sort === 'nameDesc') list.sort((a, b) => b.name.localeCompare(a.name));
  else list.sort((a, b) => (b.featured ? 1 : 0) - (a.featured ? 1 : 0));

  productGrid.innerHTML = '';
  if (!list.length) {
    const card = document.createElement('div'); card.className = 'card muted'; card.textContent = 'No products match your search.';
    productGrid.appendChild(card); return;
  }

  list.forEach(p => {
    const card = document.createElement('article');
    card.className = 'card product-card';
    card.innerHTML = `
      <div class="product-media" style="background-image:url('${p.img}')"></div>
      <div class="product-body">
        <div style="display:flex;justify-content:space-between;align-items:flex-start;gap:.5rem;">
          <h3 style="margin:0;font-size:1.05rem">${p.name}</h3>
          <div class="price">₹${p.price}</div>
        </div>
        <p class="muted" style="margin:.45rem 0;font-size:.95rem">${p.category}</p>
        <div style="display:flex;gap:.5rem;justify-content:space-between;align-items:center">
          <div style="display:flex;gap:.4rem">
            <button class="btn small add-cart">Add</button>
            <button class="btn small ghost view">View</button>
          </div>
          <div class="muted" style="font-size:.9rem">⭐ ${(p.featured ? '4.8' : '4.2')}</div>
        </div>
      </div>
    `;
    // small interactions
    card.querySelector('.view').addEventListener('click', () => alert(`${p.name}\n\nPrice: ₹${p.price}\nCategory: ${p.category}`));
    productGrid.appendChild(card);
  });
}

/* Controls binding */
searchInput.addEventListener('input', e => { productState.query = e.target.value; renderProducts(); });
categorySelect.addEventListener('change', e => { productState.category = e.target.value; renderProducts(); });
sortSelect.addEventListener('change', e => { productState.sort = e.target.value; renderProducts(); });

renderProducts();

/* ---------------------------
   Small accessibility + UX polish
-----------------------------*/
document.querySelectorAll('a[href^="#"]').forEach(a => {
  a.addEventListener('click', e => {
    const href = a.getAttribute('href');
    if (href.length > 1) {
      const el = document.querySelector(href);
      if (el) { e.preventDefault(); el.scrollIntoView({ behavior: 'smooth', block: 'start' }); }
    }
  });
});

// lightweight keyboard convenience: press 'n' to focus new task
document.addEventListener('keydown', (e) => {
  if (e.key === 'n' && document.activeElement.tagName !== 'INPUT' && document.activeElement.tagName !== 'TEXTAREA') {
    todoInput.focus();
  }
});


</html>
